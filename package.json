{
    "name": "ip-vendor-scan",
    "version": "1.1.0",
    "description": "Scans a range of IP addresses and returns a list of network names and vendors",
    "main": "dist/index.js",
    "bin": {
        "ip-vendor-scanner": "dist/index.js"
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint \"**/*.ts\"",
        "verify-release": "npm version patch && npm ci && npm run clean && concurrently --kill-others-on-fail npm:lint npm:build npm:lint",
        "prepublishOnly": "npm run verify-release"
    },
    "author": "Giles Roadnight",
    "license": "MIT",
    "devDependencies": {
        "@types/cli-table": "^0.3.0",
        "@types/command-line-args": "^5.0.0",
        "@types/command-line-usage": "^5.0.1",
        "@types/jest": "^25.2.3",
        "@types/node": "^14.0.4",
        "@types/node-fetch": "^2.5.7",
        "@types/ping": "^0.2.0",
        "@types/single-line-log": "^1.1.0",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "concurrently": "^5.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^26.0.1",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.0.0",
        "typescript": "^3.9.3"
    },
    "dependencies": {
        "@network-utils/arp-lookup": "^1.3.4",
        "cli-table": "^0.3.1",
        "command-line-args": "^5.1.1",
        "command-line-usage": "^6.1.0",
        "node-fetch": "^2.6.0",
        "ping": "^0.2.3",
        "rxjs": "^6.5.5",
        "single-line-log": "^1.1.2"
    },
    "repository": {
        "url": "https://github.com/Roaders/ip-vendor-scanner.git",
        "type": "git"
    }
}
